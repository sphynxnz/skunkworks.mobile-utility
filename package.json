{
  "name": "mobileutility",
  "version": "1.0.0",
  "description": "mobile utility",
  "main": "index.js",
  "scripts": {
    "build:clean": "rm -rf node_modules && if [ -d logs ]; then rm -rf logs; fi && mkdir logs",
    "build:test": "npm install && npm run test:util",
    "build:prod": "npm install --production",
    "test:checker": "cucumberjs --require ./test/lib ./test/ticketchecker.feature",
    "test:checker2": "cucumberjs --require ./test/lib ./test/ticketchecker2.feature",
    "test:finder": "cucumberjs --require ./test/lib ./test/storesfinder.feature",
    "test:checker:gcp": "cucumberjs --require ./test/lib ./test/ticketchecker_gcp.feature",
    "test:finder:gcp": "cucumberjs --require ./test/lib ./test/storesfinder_gcp.feature",
    "start:mock": "forever start ./node_modules/.bin/stubmatic -v -c ./test/mockserver/mockconfig.json",
    "stop:mock": "o=`forever list | grep stubmatic`; uid=`echo $o | cut -d ' ' -f 3`;echo \\\"$uid\\\"; [ \\\"$uid\\\" != \\\"\\\" ] && forever stop $uid",
    "start:util:mock": "npm run start:mock && forever start index.js --env mock",
    "stop:util:mock": "npm run stop:util && npm run stop:mock",
    "start:util:cat": "forever start index.js --env cat1",
    "start:util:prod": "forever start index.js --env production",
    "stop:util": "o=`forever list | grep index.js`; uid=`echo $o | cut -d ' ' -f 3`;echo \\\"$uid\\\"; [ \\\"$uid\\\" != \\\"\\\" ] && forever stop $uid",
    "test:util": "npm run start:util:mock && npm run -s pause && npm run test:checker && npm run -s pause && npm run test:finder && npm run -s pause && npm run stop:util:mock",
    "pause": "echo '\n===' && sleep 3s",
    "start:docker": "forever ./node_modules/.bin/stubmatic -v -c ./test/mockserver/mockconfig.json & forever index.js"
  },
  "keywords": [
    "ticket",
    "checker",
    "stores",
    "finder"
  ],
  "author": "flpcontreras",
  "license": "ISC",
  "dependencies": {
    "bunyan": "^1.8.10",
    "csvdata": "^1.4.0",
    "fast-xml-parser": "^2.4.1",
    "forever": "^0.15.3",
    "glue": "^4.1.0",
    "hapi": "^16.1.1",
    "hapi-bunyan": "^0.7.0",
    "hapi-swagger": "^7.7.0",
    "haversine": "^1.0.2",
    "inert": "^4.2.0",
    "joi": "^10.5.0",
    "lodash": "^4.17.4",
    "path": "^0.12.7",
    "soap": "^0.19.0",
    "traverse": "^0.6.6",
    "vision": "^4.1.1"
  },
  "devDependencies": {
    "assert": "^1.4.1",
    "cucumber": "^2.3.1",
    "eslint": "^4.0.0",
    "eslint-config-google": "^0.8.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-node": "^5.0.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "moment": "^2.18.1",
    "random-words": "0.0.1",
    "request-promise": "^4.2.1",
    "stubmatic": "^5.0.4"
  }
}
